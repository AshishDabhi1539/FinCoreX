package com.tss.structural.adaptor.model;

public class Biscuit implements IItems {
    private String name;
    private double price;

    public Biscuit(String name, double price) {
        this.name = name;
        this.price = price;
    }

    public String getItemName() {
        return name;
    }

    public double getItemPrice() {
        return price;
    }
}
